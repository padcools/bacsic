<!DOCTYPE>
<html >
<head>

<title></title>
<%- include ../layout/head %>
</head>

<body>
    <div class="container">
        <%- include ../layout/nav %>

        <div class="panel panel-default">
                <div class="panel-heading">
                    Add new Post
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-6">
                            <form role="form">
                                <div class="form-group">
                                    <label>Title</label>
                                    <input class="form-control" name="title" placeholder="Title">
                                </div>
                                <div class="form-group">
                                    <label>Content</label>
                                    <input class="form-control" name="content" placeholder="Content">
                                </div>
                                <div class="form-group">
                                    <label>Author</label>
                                    <input class="form-control" name="author" placeholder="Author">
                                </div>
                                <div class="form-group">
                                    <label>Created</label>
                                    <input class="form-control" name="created" placeholder="Created">
                                </div>
                                <div class="form-group">
                                    <label>Updated</label>
                                    <input class="form-control" name="updated" placeholder="Updated">
                                </div>
                                <div class="form-group">
                                    <label>Actions</label>
                                    <input class="form-control" name="actions" placeholder="Actions">
                                </div>
                            </form>
                        </div>
                        <!-- /.col-lg-6 (nested) -->
                    </div>
                    <!-- /.row (nested) -->
                </div>
                <!-- /.panel-body -->
            </div>
    </div>
</body>
</html>